{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Articles"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    }
  }
}
